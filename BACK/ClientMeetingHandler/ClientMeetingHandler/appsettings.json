{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
    "Client": {
      "NameMaxLength": 150
    },
    "Contact": {
      "CountryMaxLength": 100,
      "PhoneNumberMaxLength": 20,
      "EmailMaxLength": 100
    },
    "Localization": {
      "CountryMaxLength": 100,
      "CityMaxLength": 100,
      "StreetMaxLength": 100
    },
    "Meeting": {
      "DurationMaxLength": 10
    },
    "Note": {
      "TitleMaxLength": 50,
      "ContextMaxLength": 255
    },
    "Service": {
      "NameMaxLength": 150
    },
    "ServiceType": {
      "NameMaxLength": 150,
      "DescriptionMaxLength": 255,
      "PriceMaxLength": 10,
      "CountryMaxLength": 100
    }
  }
}
